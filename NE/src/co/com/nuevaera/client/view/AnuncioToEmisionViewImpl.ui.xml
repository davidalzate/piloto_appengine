<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
  xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"

  >
<g:SimplePanel height="100%" width="100%" >
  <g:DockLayoutPanel
    unit="EM"  height="100%" width="100%" >
    <!-- DataGrid. -->
    <g:center>
      <c:DataGrid
        ui:field='dataGrid' width="100%" height="100%" />
    </g:center>

    <!-- Pager. -->
    <g:south
      size="3">
      <g:HTMLPanel>
        <table
          style="width:100%">
          <tr>
            <td
              align='center'>
              <c:SimplePager
                ui:field='pager' />
            </td>
          </tr>
        </table>
      </g:HTMLPanel>
    </g:south>
    <g:north size="4.4">
    	<g:SimplePanel width="100%" height="57px">
    		<g:HTML wordWrap="true" width="100%" height="100%"><h><b>Anuncios</b></h></g:HTML>
    	</g:SimplePanel>
    </g:north>    
  </g:DockLayoutPanel>
  </g:SimplePanel>
</ui:UiBinder>